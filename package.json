{
  "name": "danger-plugin-gitlab",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/libs/index.cjs",
  "module": "./dist/libs/index.js",
  "types": "./dist/libs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/libs/index.js",
      "require": "./dist/libs/index.cjs"
    },
    "./dist/utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rm -rf dist/",
    "dev": "vite",
    "tsc": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "npm run tsc && npm run build:libs && npm run build:types",
    "build:libs": "vite build",
    "build:types": "tsc -p tsconfig.prod.json",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.30.1",
    "danger": "^11.2.6",
    "glob": "^10.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.30.1"
  }
}
