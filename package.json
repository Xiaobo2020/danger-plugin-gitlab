{
  "name": "danger-plugin-gitlab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist/",
    "dev": "vite",
    "tsc": "tsc",
    "test": "vitest",
    "build": "npm run tsc && npm run build:libs && npm run build:types",
    "build:libs": "vite build",
    "build:types": "tsc src/libs/*.ts --declaration --emitDeclarationOnly --declarationDir dist/",
    "coverage": "vitest run --coverage",
    "preview": "vite preview"
  },
  "devDependencies": {
    "danger": "^11.2.6",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.30.1"
  }
}
